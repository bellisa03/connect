<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <connectionStrings>
    <!--String de connexion à modifier en fonction de votre environnement: modifiez le chemin spécifié à Data Source (le chemin de mon environnement = .\SQLEXPRESS;Initial)-->
    <add name="Connect.Properties.Settings.ConnectConnectionString"
      connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=Connect;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>